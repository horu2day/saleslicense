# Manus Forge API
VITE_FRONTEND_FORGE_API_URL=https://forge.manus.ai

# 데이터베이스 연결 (PostgreSQL)
DATABASE_URL=postgresql://user:password@localhost:5432/softhub

# 토스페이먼츠 API 키 (API 개별 연동 방식)
# - 결제위젯이 아닌 SDK/API로 개별 연동할 때 사용
# - 상점아이디(MID)별로 다른 키 사용
# - 최소 결제 금액 제한 없음 (위젯은 33만원부터)
# 테스트 키: https://developers.tosspayments.com/my/api-keys
# 라이브 키: https://developers.tosspayments.com/my/api-keys (운영 전환 후)
VITE_TOSS_CLIENT_KEY=test_ck_live_xxxxxxxxxxxxxxxx
TOSS_SECRET_KEY=test_sk_live_xxxxxxxxxxxxxxxx

# Clerk 인증 설정 (필수)
# https://dashboard.clerk.com 에서 발급받은 API 키를 입력하세요
VITE_CLERK_PUBLISHABLE_KEY=pk_test_your-clerk-publishable-key
CLERK_SECRET_KEY=sk_test_your-clerk-secret-key

# 서버 포트 (선택사항)
PORT=3000
